

<table class="table table-striped">
	<thead>
		<tr>
			<th><b>Name</b></th>
			<th><b>Phone</b></th>
			<th><b>Date</b></th>
			<th><b>Barber</b></th>
			<th><b>Color</b></th>
		</tr>
	</thead>
	<tbody>
		<% @results.each do |row| %>
			<tr>
				<td><%= row['username'] %></td>
				<td><%= row['phone'] %></td>
				<td><%= row['datestamp'] %></td>
				<td><%= row['barber'] %></td>
				<td><%= row['color'] %></td>
			</tr>
		<% end %>
	</tbody>
</table>
